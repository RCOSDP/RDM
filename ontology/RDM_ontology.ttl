                             ] ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( rdm:Repository
                                            rdm:SoftwareApplication
                                          )
                            ] ;
                 rdfs:comment "Uploaded or downloaded from"@en ,
                              "アップロードもしくはダウンロード元"@ja ;
                 rdfs:label "fromLocation" .


###  https://purl.org/rdm/ontology/funder
rdm:funder rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( rdm:Grant
                                       rdm:Project
                                       rdm:Resource
                                     )
                       ] ;
           rdfs:range rdm:FundingAgency ;
           rdfs:comment "An organization that funds this Project or research activities to create this Resource."@en ,
                        "研究資金プログラム提供者"@ja ;
           rdfs:label "funder" .


###  https://purl.org/rdm/ontology/funding
rdm:funding rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( rdm:Project
                                        rdm:Resource
                                      )
                        ] ;
            rdfs:range rdm:Grant ;
            rdfs:comment "A grant program for this Project or research activities to create this Resource."@en ,
                         "研究資金プログラム"@ja ;
            rdfs:label "funding" .


###  https://purl.org/rdm/ontology/hasMetadata
rdm:hasMetadata rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf rdm:isRelatedTo ;
                owl:inverseOf rdm:isMetadataFor ;
