            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( rdm:Resource
                                        rdm:SoftwareApplication
                                      )
                        ] ;
            rdfs:range rdm:Person ;
            rdfs:comment "The creator/author of this Resource. "@en ,
                         "リソースの作成者"@ja ;
            rdfs:label "creator" .


###  https://purl.org/rdm/ontology/dataAccessRightsInformation
rdm:dataAccessRightsInformation rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf rdm:accessRightsInformation ;
                                rdfs:domain rdm:DataManagementPlan ;
