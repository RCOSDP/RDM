###  https://purl.org/rdm/ontology/experimenter
rdm:experimenter rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf rdm:contributor ;
                 rdfs:domain rdm:Experiment ;
                 rdfs:range rdm:Person ;
                 rdfs:comment "The practitioner, performer and executor of this Experiment."@en ,
                              "実験の実施者、実行者"@ja ;
                 rdfs:label "experimenter" .


###  https://purl.org/rdm/ontology/fromLocation
rdm:fromLocation rdf:type owl:ObjectProperty ;
