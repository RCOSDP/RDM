                rdfs:range rdm:MetadataDocument ;
                rdfs:comment "A Resource which has additional metadata of this Class."@en ,
                             "主語クラスの追加メタデータを持つ Resource"@ja ;
                rdfs:label "hasMetadata" ;
                rdfs:seeAlso "https://datacite-metadata-schema.readthedocs.io/en/4.5/appendices/appendix-1/relationType/#continues" .


###  https://purl.org/rdm/ontology/hasPart
rdm:hasPart rdf:type owl:ObjectProperty ;
